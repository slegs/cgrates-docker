{

// Real-time"general": {
       	"node_id": "CGRATES_NAME",
	"reply_timeout": "5s",
        "logger": "CGRATES_LOGGER",
        "log_level": CGRATES_LOG_LEVEL,
},


"listen": {
	"rpc_json": "0.0.0.0:2012",
	"rpc_gob": "0.0.0.0:2013",
	"http": "0.0.0.0:2080",
},


"data_db": {
        "db_type": "mongo",
        "db_host": "MONGO_HOST",
        "db_name": "MONGO_DATADB",
        "db_port": 27017,
},

"stor_db": {
        "db_type": "mongo",
        "db_host": "MONGO_HOST",
        "db_name": "MONGO_STORDB",
        "db_port": 27017,
},

"rpc_conns": {
  "internalconn": {
		"strategy": "*first",
		"conns": [
			{"address": "127.0.0.1:2012", "transport":"*json"},
		],
	},
  "firstconn": {
		"strategy": "*first",
		"conns": [CGRATES_CONNS],
	},
  "roundrobinconn": {
		"strategy": "*round_robin",
		"conns": [CGRATES_CONNS],
	},
  "randomconn": {
		"strategy": "*random",
		"conns": [CGRATES_CONNS],
	},
  "ratioconn": {
		"strategy": "*ratio",
		"conns": [CGRATES_CONNS],
	},
  "allconn": {
		"strategy": "*broadcast",
		"conns": [CGRATES_CONNS],
	},
},


"schedulers": {
	"enabled": true,
},

"sessions": {
	"enabled": CGRATES_SESSION_ENABLED,
	"rals_conns": ["firstconn"],
	"cdrs_conns": ["firstconn"],
	"resources_conns": ["firstconn"],
	"suppliers_conns": ["firstconn"],
	"attributes_conns": ["firstconn"],
	"stats_conns": ["firstconn"],
	"thresholds_conns": ["firstconn"],
	"chargers_conns": ["firstconn"],
	"debit_interval": "5s",
	"channel_sync_interval":"7s",
},


}
